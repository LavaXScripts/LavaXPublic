local Library = loadstring(game:HttpGet("https://raw.githubusercontent.com/xHeptc/Kavo-UI-Library/main/source.lua"))()
local Window = Library.CreateLib("LavaX Public/Free", "BloodTheme")

â€” Blatant
local Blatant = Window:NewTab("Blatant")
local BlatantSection = Blatant:NewSection("Blatant")


BlatantSection:NewButton("InfJumps", "Good Crossing Bases That Are CloseTogether", function()
    loadstring(game:HttpGet("https://cdn.wearedevs.net/scripts/Infinite%20Jump.txt"))()
end)


BlatantSection:NewButton("Nuker", "Automatically Breaks Beds", function()
    print("Clicked")
end)


BlatantSection:NewSlider("Slide To 23 Speed", "Makes You Go Zoom Zoom When Max",23, 0, function(s) (MaxValue) 23 | 0 (MinValue)
    game.Players.LocalPlayer.Character.Humanoid.WalkSpeed = s
end)


BlatantSection:NewButton("Velocity", " Disables  Knockback", function()
    loadstring(game:HttpGet("https://raw.githubusercontent.com/LavaXScripts/Velocity/main/Velocity",true))();
end)
